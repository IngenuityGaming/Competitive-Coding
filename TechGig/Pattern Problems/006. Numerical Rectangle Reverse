#include <iostream>
using namespace std;
int main()
{
	int n,i,j;
	cin>>n;
	for(i=n;i>=1;i--)
	{
		for(j=1;j<=5;j++)
		{
			if(j<5)
				cout<<i<<" ";
			else
				cout<<i;
		}
		if(i>1)
			cout<<"\n";
	}
  return 0;
}
