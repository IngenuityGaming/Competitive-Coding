#include <iostream>
using namespace std;
int main()
{
	int n,i,j;
	cin>>n;
	for(i=1;i<=n;i++)
	{
		for(j=5;j>=1;j--)
		{
			if(j>1)
				cout<<j<<" ";
			else
				cout<<j;
		}
		if(i<n)
			cout<<"\n";
	}
  return 0;
}
