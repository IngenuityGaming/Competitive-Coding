#include <iostream>
using namespace std;
int main()
{
	int n,i,j;
	cin>>n;
	for(i=1;i<=n;i++)
	{
		for(j=1;j<=5;j++)
		{
			if(j<5)
				cout<<"*"<<" ";
			else
				cout<<"*";
		}
		if(i<n)
			cout<<"\n";
	}
 	return 0;
}
